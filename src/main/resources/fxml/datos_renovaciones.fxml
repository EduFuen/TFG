<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Pos?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.comproOro.gestion.controller.DatosRenovacionesController">
    <children>

        <TableView fx:id="renovacionesTableView" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.bottomAnchor="50">
            <columns>
                <TableColumn fx:id="idContratoColumn" text="ID Contrato" />
                <TableColumn fx:id="fechaRenovacionColumn" text="Fecha Renovación" />
                <TableColumn fx:id="fechaFinRenovacionColumn" text="Fecha Fin Renovación" />
                <TableColumn fx:id="versionColumn" text="Versión" />
                <TableColumn fx:id="importeColumn" text="Importe" />
            </columns>
        </TableView>


        <Button text="Cancelar" fx:id="cancelButton" onAction="#handleCancelar" AnchorPane.bottomAnchor="10" AnchorPane.rightAnchor="10" />
    </children>
</AnchorPane>
